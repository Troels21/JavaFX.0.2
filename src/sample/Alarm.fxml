            <TextField fx:id="ekgMax" alignment="CENTER_RIGHT" layoutX="512.0" layoutY="172.0" prefHeight="25.0" prefWidth="81.0" text="1337" />
            <TextField fx:id="ekgMin" alignment="CENTER_RIGHT" layoutX="512.0" layoutY="198.0" prefHeight="25.0" prefWidth="81.0" text="420" />
            <Button layoutX="525.0" layoutY="319.0" mnemonicParsing="false" onAction="#updateAlarm" text="Update" />
      </children>
    </AnchorPane>
  </children>
</VBox>
