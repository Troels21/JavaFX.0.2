<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerAlarm">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="140.0" layoutY="150.0" text="Pulse" />
            <Label layoutX="252.0" layoutY="150.0" text="Temperature" />
            <Label layoutX="401.0" layoutY="150.0" text="SpO2 %" />
            <Label layoutX="539.0" layoutY="150.0" text="EKG?" />
            <Label layoutX="31.0" layoutY="176.0" text="Max" />
            <Label layoutX="32.0" layoutY="202.0" text="Min" />
            <Label layoutX="278.0" layoutY="21.0" text="Alarm">
               <font>
                  <Font size="35.0" />
               </font>
            </Label>
            <TextField fx:id="pulseMax" alignment="CENTER_RIGHT" layoutX="120.0" layoutY="172.0" prefHeight="10.0" prefWidth="68.0" />
            <TextField fx:id="pulseMin" alignment="CENTER_RIGHT" layoutX="120.0" layoutY="198.0" prefHeight="25.0" prefWidth="68.0" />
            <TextField fx:id="tempMax" alignment="CENTER_RIGHT" layoutX="245.0" layoutY="172.0" prefHeight="25.0" prefWidth="81.0" />
            <TextField fx:id="tempMin" alignment="CENTER_RIGHT" layoutX="245.0" layoutY="198.0" prefHeight="25.0" prefWidth="81.0" />
            <TextField fx:id="SpO2Max" alignment="CENTER_RIGHT" layoutX="382.0" layoutY="172.0" prefHeight="25.0" prefWidth="81.0" />
            <TextField fx:id="SpO2Min" alignment="CENTER_RIGHT" layoutX="382.0" layoutY="198.0" prefHeight="25.0" prefWidth="81.0" />
            <TextField fx:id="ekgMax" alignment="CENTER_RIGHT" layoutX="512.0" layoutY="172.0" prefHeight="25.0" prefWidth="81.0" />
            <TextField fx:id="ekgMin" alignment="CENTER_RIGHT" layoutX="512.0" layoutY="198.0" prefHeight="25.0" prefWidth="81.0" />
            <Button layoutX="525.0" layoutY="319.0" mnemonicParsing="false" onAction="#updateAlarm" text="Update" />
      </children>
    </AnchorPane>
  </children>
</VBox>
